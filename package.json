{
	"name": "tree-view-max-call-stack-error",
	"displayName": "TreeViewMaxCallStackError",
	"description": "Tree View Max Call Stack Error",
	"publisher": "RedCMD",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.94.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RedCMD/TmLanguage-Syntax-Highlighter.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"build": "npm run compile",
		"package": "vsce package",
		"watch": "tsc -watch -p ./",
		"compile": "tsc -p ./"
	},
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "TreeView",
					"title": "TreeView",
					"icon": "list-tree.svg"
				}
			]
		},
		"views": {
			"TreeView": [
				{
					"id": "TreeView",
					"name": "TreeView",
					"contextualTitle": "TreeView",
					"visibility": "visible"
				}
			]
		}
	},
	"devDependencies": {
		"@types/vscode": "^1.87.0",
		"@vscode/vsce": "^3.1.1",
		"ts-loader": "^9.5.1",
		"typescript": "^5.6.2"
	}
}
